            <string>xspeed += (keyboard_check(vk_right)-keyboard_check(vk_left));
yspeed += (keyboard_check(vk_down)-keyboard_check(vk_up));
while(place_meeting(x+xspeed,y,obj_block))
{
    xspeed -= sign(xspeed);
}
while(place_meeting(x,y+yspeed,obj_block))
{
    yspeed -= sign(yspeed);
}
x += xspeed;
y += yspeed;
xspeed /= 1.5;
yspeed /= 1.5;
if(xspeed &gt; 8) xspeed = 8;
if(yspeed &gt; 8) yspeed = 8;
if(xspeed &lt; -8) xspeed = -8;
if(yspeed &lt; -8) yspeed = -8;
if(mouse_check_button(mb_left))
{
    switch(currentitemslot)
    {
        case 0:
            if(can_shoot &amp;&amp; mouse_check_button_pressed(mb_left))
            {
                var inst = instance_create(x,y,obj_grenade);
                with(inst)
                {
                    var _dir = point_direction(obj_player.x,obj_player.y,mouse_x,mouse_y);
                    dir = _dir;
                    spd = 12;
                    image_angle = _dir;
                }
                can_shoot = false;
                alarm[0] = 30;
            }
            break;
        case 1:
            if(can_shoot &amp;&amp; mouse_check_button_pressed(mb_left))
            {
                var inst = instance_create(x+lengthdir_x(44,arm_dir),y+lengthdir_y(9,arm_dir),obj_bullet);
                with(inst)
                {
                    direction = other.arm_dir;
                    image_angle = other.arm_dir;
                    speed = 24;
                }
                can_shoot = false;
                alarm[0] = 5
            }
        case 2:
            if(can_shoot)
            {
                var inst = instance_create(x+lengthdir_x(44,arm_dir),y+lengthdir_y(9,arm_dir),obj_bullet);
                with(inst)
                {
                    direction = other.arm_dir;
                    image_angle = other.arm_dir;
                    speed = 18;
                }
                can_shoot = false;
                alarm[0] = 3.75
            }
    }
}
if(mouse_wheel_up())
{
    currentitemslot += 1;
}
if(mouse_wheel_down())
{
    currentitemslot -= 1;
}
var size_ = ds_list_size(inventory_list);
while(currentitemslot &gt; size_)
{
    currentitemslot -= size_;
}
while(currentitemslot &lt; 0)
{
    currentitemslot += size_;
}
dir = point_direction(x,y,mouse_x,mouse_y);
arm_dir = dir+(angle_difference(arm_dir,dir)/2);
leg_dir = dir+(angle_difference(leg_dir,arm_dir)/2);
var can_move_ = xspeed &gt; 1.5 || yspeed &gt; 1.5 || xspeed &lt; -1.5 || yspeed &lt; -1.5;
if(can_move_ || arm_index &gt; 0)
{
    arm_index++;
    if(arm_index &gt;= arm_maxindex)
    {
        arm_index -= arm_maxindex;
    }
}
if(x &gt; room_width)
{
    obj_view.x -= room_width;
    x -= room_width;
}
if(x &lt; 0)
{
    obj_view.x += room_width;
    x += room_width;
}
if(y &gt; room_height)
{
    obj_view.y -= room_height;
    y -= room_height;
}
if(y &lt; 0)
{
    obj_view.y += room_height;
    y += room_height;
}
